{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/import_dump",
            "args": ["-threads", "12", "-n", "5000", "../arwiki-20200420-pages-articles-multistream.xml.bz2"],
            "stopAtEntry": false,
            //"cwd": "${workspaceFolder}",
            "cwd": "/data2/wikipedia/arwiki/2020",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Dump2db - Get all Wikipedia pages and save them into sqlite db",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/dump2db",
            "args": ["-thread", "12", "-milestone", "100", "-batch", "1", "-wikisite", "http://arwiki.tyo.com.au", "-wikiroot", "/wiki/", "../arwiki-20200420-pages-articles-multistream.xml.bz2"],
            "stopAtEntry": false,
            //"cwd": "${workspaceFolder}",
            "cwd": "/data2/wikipedia/arwiki/2020",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}